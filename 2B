#include <iostream>
#include <stdlib.h>
#include <ctime>
using namespace std;

int servicio_tel[12000][3];
void llamarST(){
    for (int i=0; i<12000; i++){
        servicio_tel [i][0]=((rand()%2+6)*10000000)+(rand()%(9999999-1000000+1)+1000000);
        servicio_tel [i][1] = (rand()%5)+1;
        servicio_tel [i][2] = rand()%10000;
    }
}
void imprimirST(){
    cout<<"TELEFONO:     PLAN:     IDCLIENTE: "<<endl;
    for (int i=0; i<12000; i++){
        cout<<servicio_tel [i][0]<<"       "<<servicio_tel [i][1]<<"       "<<servicio_tel [i][2]<<endl;
    }
}

int main(){
    
    srand(time(0));
    llamarST();
    imprimirST();
    
    return 0;
}
