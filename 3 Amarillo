#include <iostream>
#include <stdlib.h>
#include <ctime>
#include <iomanip>
using namespace std;

int llamada[10000][14];
void llamar(){
    for (int i=0; i<10000; i++){
        llamada [i][0] = 2024;
        llamada [i][1] = 1+rand()%12;
        llamada [i][2] = 1+rand()%30;
        llamada [i][3] = 1+rand()%24;
        llamada [i][4] = 1+rand()%60;
        llamada [i][5] = rand()%12000;
        llamada [i][6] = rand()%12000;
        
        while (llamada[i][5] == llamada[i][6]){
            llamada[i][6] = rand()%12000;
        }
        
        llamada [i][7] = 1+rand()%120;
       /* llamada [i][8] =
        llamada [i][9] = 
        llamada [i][10] = 
        llamada [i][11] = 
        llamada [i][12] = 
        llamada [i][13] = */
    }
}
void imprimir(){
    cout << left
         << setw(12) << "IdLlamada"
         << setw(6)  << "Año"
         << setw(6)  << "Mes"
         << setw(6)  << "Día"
         << setw(6)  << "Hora"
         << setw(6)  << "Min"
         << setw(13) << "Tel. Origen"
         << setw(13) << "Tel. Destino"
         << setw(10) << "Dur(min)"
         << endl;


    for (int i = 0; i < 10000; i++) {
    cout << left
         << setw(12) << i
         << setw(6)  << llamada[i][0]  // Año
         << setw(6)  << llamada[i][1]  // Mes
         << setw(6)  << llamada[i][2]  // Día
         << setw(6)  << llamada[i][3]  // Hora
         << setw(6)  << llamada[i][4]  // Min
         << setw(13) << llamada[i][5]  // Tel. Origen
         << setw(13) << llamada[i][6]  // Tel. Destino
         << setw(10) << llamada[i][7]  // Duración
         << endl;
}
}

int main(){
    
    srand(time(0));
    llamar();
    imprimir();
    
    return 0;
}
